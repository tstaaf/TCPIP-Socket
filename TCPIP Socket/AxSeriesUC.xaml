<UserControl x:Class="TCPIP_Socket.AxSeriesUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TCPIP_Socket"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="590">
    <Grid>
        <StackPanel>
            <Label Content="Message name:"/>
            <TextBox x:Name="MessageName" Margin="10"/>
            <Label Content="Command:"/>
            <TextBox x:Name="MessageInput" Margin="10"/>
        </StackPanel>
    </Grid>
</UserControl>
