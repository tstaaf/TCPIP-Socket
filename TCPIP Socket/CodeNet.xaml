<Window x:Class="TCPIP_Socket.CodeNet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCPIP_Socket"
        mc:Ignorable="d"
        Title="CodeNet" Height="450" Width="450">
    <Grid Margin="10">
        <StackPanel>
            <Label Content="Message name:"/>
            <TextBox x:Name="MessageName" Margin="10"/>
            <Label Content="Command:"/>
            <TextBox x:Name="MessageInput" Margin="10"/>
        </StackPanel>
        


    </Grid>
</Window>
